<Alloy> 
	<TabGroup id="tabGroup"  tabsTintColor="#10844D"  onOpen="doOpen" backgroundColor="#000000"  >
		<Tab id="dashboardTab"  tintColor="black" title="Dashboard" icon="/images/icons/icon_notice.png" activeIcon="/images/icons/icon_notice_active.png" iconIsMask="false" activeIconIsMask="false"  activeIconIsMask="false">
			<Window id="win1" title="Dashboard" backgroundColor="#F1F1F1" > 
				<LeftNavButton platform="ios">
				    <View id="btnLogout" left="0"  width="30" height="35">
				     	<ImageView left='0'  width="Ti.UI.SIZE"   image="/images/logout.png" />
				    </View>
				</LeftNavButton> 
				<Require id="dashboardDetailsView" src="homepage/dashboard" ></Require> 
			</Window>
		</Tab>
		<Tab color="white" title="School" icon="/images/icons/icon_school.png" activeIcon="/images/icons/icon_school_active.png" iconIsMask="false" activeIconIsMask="false"  activeIconIsMask="false">
			<Window title="School" backgroundColor="#F1F1F1" > 
				<Require id="schoolDetailsView" src="homepage/education"></Require>
			</Window>
		</Tab>
		<Tab title="Tuition Centre" icon="/images/icons/icon_tuition.png" activeIcon="/images/icons/icon_tuition_active.png" iconIsMask="false" activeIconIsMask="false"  activeIconIsMask="false">
			<Window title="Tuition Centre"  backgroundColor="#F1F1F1" > 
				<Require id="tuitionDetailsView" src="homepage/education"></Require>
			</Window>
		</Tab>
		<Tab title="Others" icon="/images/icons/icon_others.png" activeIcon="/images/icons/icon_others_active.png" iconIsMask="false" activeIconIsMask="false"  activeIconIsMask="false">
			<Window title="Others" backgroundColor="#F1F1F1" > <!--backgroundImage="/images/home_background.jpg" -->
				<Require id="othersDetailsView" src="homepage/others"></Require>
			</Window>
		</Tab>
	 
	</TabGroup>
</Alloy>